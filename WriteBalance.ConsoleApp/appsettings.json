{
  "ApiSettings": {
    "PostIsUniqueUrl": "api/common/postIsUnique",
    "PostBalanceSheetUrl": "api/balancesheet/post",
    "ControllerName": 13,
    "RetryCount": 3,
    "RetryDelaySeconds": 2
  },
  "AuthConfig": {
    "AuthEndpointUrl": "api/dominoendpoint/login"
  },
    "Serilog": {
      "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
      "MinimumLevel": {
        "Default": "Information",
        "Override": {
          "Microsoft": "Information",
          "System": "Information"
        }
      },
      "WriteTo": [
        { "Name": "Console" },
        {
          "Name": "File",
          "Args": {
            "path": "logs/log-.txt",
            "rollingInterval": "Day",
            "retainedFileCountLimit": 7
          }
        }
      ],
      "Enrich": [ "FromLogContext" ],
      "Properties": {
        "Application": "WriteBalance"
      }
    }
  }
